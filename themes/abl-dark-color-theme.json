{
  "name": "ABL Dark",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "ablMap": "#869cff",
    "ablFunc": "#f1fa8c",
    "ablLogic": "#f8c57c",
    "ablFunctionDecl": "#957DAD",
    "ablFunctionEnd": "#957DAD",
    "ablFunctionCall": "#8be9fd"
  },
  "colors": {},
  "tokenColors": [
    {
      "name": "ABL Comment",
      "scope": ["comment", "comment.line", "comment.line.number-sign"],
      "settings": { "foreground": "#6A9955", "fontStyle": "italic" }
    },
    {
      "scope": "constant.language.boolean.abl",
      "settings": { "foreground": "#50fa7b" }
    },
    {
      "scope": "constant.language.nullish.abl",
      "settings": { "foreground": "#50fa7b" }
    },
    {
      "name": "ABL Numeric",
      "scope": ["constant.numeric.abl"],
      "settings": { "foreground": "#68ffeb" }
    },
    {
      "name": "ABL Index",
      "scope": ["variable.other.index.abl"],
      "settings": { "foreground": "#bd93f9" }
    },
    {
      "name": "ABL Control Keywords",
      "scope": ["keyword.control"],
      "settings": { "foreground": "#ff79c6" }
    },
    {
      "name": "ABL Get Set",
      "scope": ["keyword.other.assignment"],
      "settings": { "foreground": "#f1fa8c" }
    },
    {
      "name": "ABL Types",
      "scope": ["storage.type"],
      "settings": { "foreground": "#64BDB4" }
    },
    {
      "name": "ABL Map",
      "scope": ["support.class.map"],
      "settings": { "foreground": "#f1fa8c" }
    },
    {
      "name": "ABL Writer",
      "scope": ["support.function.writer"],
      "settings": { "foreground": "#ff5555" }
    },
    {
      "name": "ABL Token Access",
      "scope": ["support.class.token", "support.class.meta"],
      "settings": { "foreground": "#D69D43" }
    },
    {
      "name": "ABL Token/Meta Properties/ABL Reference Operator",
      "scope": ["variable.other.property.token", "variable.other.property.meta", "keyword.operator.reference"],
      "settings": { "foreground": "#FF7F50" }
    },
    {
      "name": "ABL General Functions",
      "scope": ["support.function"],
      "settings": { "foreground": "#f1fa8c" }
    }
  ]
}